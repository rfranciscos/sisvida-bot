# Sisvida Bot Configuration
# Copy this file to .env and fill in your actual credentials

# Your Sisvida login credentials
SISVIDA_USERNAME=your_username_here
SISVIDA_PASSWORD=your_password_here

# Set to 'true' to run browser in headless mode (no visible window)
HEADLESS=false

# TCP Server Configuration
# Port for the URIT-5160 analyzer to connect to
URIT5160_PORT=8080

# Port for the URIT-8031 analyzer to connect to  
URIT8031_PORT=8081

# Legacy TCP_PORT (for backward compatibility)
TCP_PORT=3000 